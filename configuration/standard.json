{
  "sources": {

  },
  "paths": {
    "staging": ".archive_vault/staging",
    "cache": ".archive_vault/cache",
    "archives": "./archive_vault"
  },
  "rules": {
    "patternMarkers": {
      "separator": "/",
      "version": "@",
      "source": "$"
    },
    "ignoreFiles": [
      ".git",
      "CVS",
      ".svn",
      ".hg",
      ".*.swp",
      ".*.tmp",
      "*.log",
      ".DS_Store",
      "node_modules",
      "bower_components",
      "archives",
      "package-lock.json"
    ],
    "configurationPriority": [
      "bower.json",
      "package.json",
      "component.json",
      "bauble.json"
    ]
  }
}
