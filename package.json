{
  "name": "barrel",
  "description": "Manage components packaged in zip files",
  "version": "0.1.0",
  "homepage": "https://github.com/mwjaworski/barrel",
  "author": {
    "name": "Michael Jaworski",
    "url": "https://github.com/mwjaworski",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mwjaworski/barrel.git"
  },
  "bugs": {
    "url": "https://github.com/mwjaworski/barrel/issues"
  },
  "license": "MIT",
  "keywords": [
    "cli",
    "command line"
  ],
  "preferGlobal": true,
  "bin": {
    "barrel": "bin/index.js"
  },
  "main": "bin/index.js",
  "engines": {
    "node": ">=6"
  },
  "ignore": [
    "node_modules",
    "reports",
    "test",
    "/.*",
    "/*.js",
    "/*.json",
    "/*.yml",
    "/*.bkp"
  ],
  "scripts": {
    "format": "node_modules/.bin/prettier --write bin/**/*.js",
    "test": "node_modules/.bin/ava --timeout=3s test/test.js",
    "coverage": "./node_modules/.bin/nyc npm run test",
    "coveralls": "npm run coverage"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "cli-color": "1.2.0",
    "cli-table": "0.3.1",
    "commander": "2.11.0",
    "jschardet": "^1.5.1",
    "jsonfile": "4.0.0",
    "jszip": "^3.1.4",
    "lodash": "4.17.4",
    "nyc": "^11.2.1",
    "semver-compare": "1.0.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "prettier": "^1.7.4"
  },
  "ava": {
    "files": [
      "bin/**/*.js"
    ],
    "source": [
      "bin/**/*.js"
    ],
    "failWithoutAssertions": true,
    "powerAssert": false,
    "concurrency": 5,
    "failFast": true,
    "tap": true
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "bin/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./reports/coverage"
  }
}
